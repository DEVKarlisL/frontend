{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run Django Backend",
			"type": "shell",
			"command": "python backend/manage.py runserver",
			"isBackground": true,
			"problemMatcher": [
				"$python"
			],
			"group": "build"
		},
		{
			"label": "Run Frontend Vite Dev Server",
			"type": "shell",
			"command": "npm run dev",
			"isBackground": true,
			"problemMatcher": [
				"$vite"
			],
			"group": "build"
		}
	]
}